<div class="container">
<h2>Instructors/Students</h2>

<% @students.each do |student| %>
  <% if student.admin %>
    <div>
      <p>Instructor: <%= link_to student.name, student_path(student) %></p>
    </div>
  <% else %>
    <div>
      <p>Student: <%= link_to student.name, student_path(student) %></p>
    </div>
  <% end %>

<% end %>

</div> <!--container  -->
